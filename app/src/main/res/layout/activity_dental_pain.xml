<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:background="@drawable/dentalback"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DentalPain">

    <ImageView
        android:id="@+id/dpImg"
        android:layout_width="260dp"
        android:layout_height="260dp"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="130dp"
        android:layout_marginBottom="75dp"
        android:layout_toStartOf="@+id/dpBtnId"
        android:src="@drawable/dental"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="83dp"
        tools:layout_editor_absoluteY="105dp" />

    <ImageButton
        android:id="@+id/dpBtnId"
        android:layout_width="200dp"
        android:layout_height="70dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="130dp"
        android:layout_marginBottom="172dp"
        android:background="@drawable/pain"
        android:transitionName="b2"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="397dp"
        tools:layout_editor_absoluteY="139dp" />


</RelativeLayout>