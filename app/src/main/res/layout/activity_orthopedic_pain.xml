<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:background="@drawable/orthoback"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".OrthopedicPain">

    <ImageView
        android:id="@+id/oImg"
        android:layout_width="260dp"
        android:layout_height="260dp"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="130dp"
        android:layout_marginBottom="75dp"
        android:layout_toStartOf="@+id/oBtnId"
        android:src="@drawable/ortho"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="83dp"
        tools:layout_editor_absoluteY="105dp" />

    <ImageButton
        android:transitionName="b3"
        android:id="@+id/oBtnId"
        android:layout_width="200dp"
        android:layout_height="70dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="130dp"
        android:layout_marginBottom="172dp"
        android:background="@drawable/pain"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="397dp"
        tools:layout_editor_absoluteY="139dp" />

</RelativeLayout>